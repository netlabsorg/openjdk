	types=($(SIGNED_REPRODUCERS)) ; \
	for which in "$${types[@]}" ; do \
	  pushd $(JNLP_TESTS_SRCDIR)/$$which/ ; \
	  find .  -maxdepth 1 -mindepth 1 | sed "s/.\/*//" > $(abs_top_builddir)/junit-jnlp-dist-$$which.txt ; \
	  popd ; \
	done ; \
	mkdir -p stamps && \
	touch $@

